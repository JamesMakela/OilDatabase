    <script type="text/javascript">       
        //Bug ID:8664-R2 - QA 415 - Country homepage - latest news and updates link should open a new tab
        //Updating all the link's target to "_blank" if the url starts with http/https
        $(document).ready(function()
        {
            // #13418 - Subtopiclist page Intro hero link GA Tag
            $('.subtopiclist-template .hero-description a').each(function()
            {
                $(this).attr('onclick','dataLayer.push({\'event\':\'trackEvent\',\'category\':\'subtopic list pages\',\'action\':\'click - intro hero link\',\'label\':\''+$(this).attr('href')+'\',\'contentSection\': \'\', \'contentSubSection\': \'\', \'contentTopic\': \'\'});');
            });

            // #13418 - Article page SubNav GA Tag
            $('.copy ._nav-editorial-side-wrap .nav-editorial-side ul li a').each(function()
            {
                var currentURL = window.location.href.split("#")[0];
                $(this).attr('onclick','dataLayer.push({\'event\':\'trackEvent\',\'category\':\'article page\',\'action\':\'click - sub nav\',\'label\':\''+ currentURL+ $(this).attr('href')+'\',\'contentSection\': \'\', \'contentSubSection\': \'\', \'contentTopic\': \'\'});');
            });

            //Loop through all page anchor tag and set target=_blank if the domain is not same as current domain.
            $('a').each(function()
            {
                //comparing the site domain with anchor tag url domain.
                if(this.host.toString() !=location.host.toString())
                {
                    $(this).attr('target','_blank');    

                    //Track-it #13418 - Implementing GA Tag push for outbound liks throughout the page
                    var existingOnclickGATagValue = $(this).attr('onclick');

                    if($(this).attr('href') != undefined && $(this).attr('href') != null )
                    {
                        if(existingOnclickGATagValue != undefined)
                            $(this).attr('onclick', existingOnclickGATagValue.replace(';','') + '; dataLayer.push({\'event\': \'trackEvent\', \'category\': \'outbound links\', \'action\': \'click\', \'label\': \''+ ($(this).attr('href')).replace(new RegExp("'","g"),"")+'\',\'contentSection\': \'\', \'contentSubSection\': \'\', \'contentTopic\': \'\'});');   
                        else 
                            $(this).attr('onclick', 'dataLayer.push({\'event\': \'trackEvent\', \'category\': \'outbound links\', \'action\': \'click\', \'label\': \''+ ($(this).attr('href')).replace(new RegExp("'","g"),"") +'\',\'contentSection\': \'\', \'contentSubSection\': \'\', \'contentTopic\': \'\'});');   
                    }
                }
            });
            //setting mailto link to be in target "_self"
            $('a[href^=mailto]').attr('target','_self'); 

            $('.video-player').on('mousedown', function (event) {
                var video = jQuery(this).attr("data-vid-id") || "";
                Webtrends.multiTrack({
